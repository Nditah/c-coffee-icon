{
  "name": "c-coffee-icon",
  "version": "0.0.2",
  "description": "A multi-color svg icon as a web component",
  "module": "dist/esm/index.js",
  "main": "dist/c-coffee-icon.js",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "stencil build",
    "start": "stencil build --dev --watch --serve",
    "test": "jest",
    "test.watch": "jest --watch"
  },
  "dependencies": {},
  "devDependencies": {
    "@stencil/core": "^0.12.4",
    "@stencil/sass": "^0.1.0",
    "@types/jest": "^23.3.1",
    "jest": "^23.4.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/agustinhaller/c-coffee-icon.git"
  },
  "author": "Agustin Haller",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/agustinhaller/c-coffee-icon/issues"
  },
  "homepage": "https://github.com/agustinhaller/c-coffee-icon",
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/@stencil/core/testing/jest.preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  }
}
